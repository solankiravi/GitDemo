pipeline {
    agent any
    stages {
        stage('Checkout') {
                steps {
                        echo 'Hi, Checkout'
			
                }
        }
	    stage('Build'){
		    
		steps {
			echo "Hello, Build Completed"
        }
	    }
        stage('Poll') {
                
                steps {
			echo "Poll Successful"
                        }
        }
        
    }
}
